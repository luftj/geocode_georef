[PIPELINE]
use_cb=true
cb_percent=5
use_colour_sep=false

[IMAGEPROC]
# black value is influenced by colour balancing
black_threshold=50

[TESSERACT]
lang=deu
tessedit_char_whitelist=abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789
psm=7
tessdata_path=/usr/share/tesseract-ocr/tessdata/

[GEOCODING]
name_minlength=5
max_alternatives=40
geocoder_source=osm
countrycode=DE

[GEOREF]
# map extent in units of used crs. default: degrees. e.g. 1Â° lat for 1:100k usgs
max_map_extent=0.15

transform=tps
